<% if object.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-danger">
      <%= I18n.t :form_error_title, errors_number: object.errors.count %>.
    </div>
    <ul>
    <% object.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>